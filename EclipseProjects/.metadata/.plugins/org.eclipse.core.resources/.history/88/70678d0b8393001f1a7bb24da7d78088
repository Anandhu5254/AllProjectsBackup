package loopingexamples;

public class Example {

public static void main(String...args)
{
int num=7;
int count=0;
if(num==1||num==0)
{
	System.out.println("not a prime number");
}
for(int i=2; i<=num; i++)
{
  if(num%i==0)
  {
	count++;  
  }
  {
	  System.out.println("The given number is prime");
  }
  else
  {
	  System.out.println("not a prime number");
  }
}
}
}
	





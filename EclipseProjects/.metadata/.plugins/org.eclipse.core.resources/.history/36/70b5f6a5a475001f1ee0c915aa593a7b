package loopingexamples;

public class primenumber {

public static void main(String...args)
{
	int num=5;
	int count=0;
	if(num==0||num==1)
	{
		System.out.println("not a prime number");
	}
	for(int i=2; i<=num; i++)
	{
		if(num%i==0)
		{
			count++;
		}
	}
	if(count==1)
	{
		System.out.println("prime number");
	}
	else
	{
		System.out.println("not a prime number");
	}
}
}





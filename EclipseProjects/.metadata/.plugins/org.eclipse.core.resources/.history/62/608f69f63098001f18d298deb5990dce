package loopingexamples;

interface Printable {

void print();
}
interface Showable{
	void show();
}
class A7 implements Printable,Showable{
	
	public void print()
	{
		System.out.print("Hello");
	}
	public void show()
	{
		System.out.println("Welcome");
	}
public static void main(String...args) {
	
	A7 obj= new obj();
	obj.print();
	obj.show();
}
}


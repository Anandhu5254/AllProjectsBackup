package loopingexamples;

public class Example {

public static void main(String...args)
{	
 int a=343;
int num=343;
int rev=0;
while(num>0)
{
	int rem=num%10;
	rev=rev*10+rem;
	num=num/10;
	
}
if(a==rev)
{
 System.out.println("palindrome number");
}
else
{
	System.out.println("Not a palindrome number");
}
}
}
	




